<?xml version="1.0" encoding="utf-8"?>
<particles>


    <texture>art/textures/particles/trail.png</texture>

    <blend mode="over"/>

    <constant name="emissionrate" value="400.0"/>

    <uniform name="lifetime" min="0.2" max="0.2"/>


    <uniform name="angle" min="-0.5" max="0.5"/>


    <uniform name="velocity.x" min="0.0" max="0.0"/>

    <uniform name="velocity.y" min="0.0" max="0.0"/>

    <uniform name="velocity.z" min="0.0" max="0.0"/>

    <uniform name="velocity.angle" min="-0.5" max="0.5"/>
    <uniform name="size" min="0.3" max="0.3"/>


    <uniform name="color.r" min="0.2" max="0.2"/>

    <copy name="color.g" from="color.r"/>

    <copy name="color.b" from="color.r"/>


</particles>
